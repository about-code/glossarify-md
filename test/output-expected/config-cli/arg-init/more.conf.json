{
  "$schema": "https://raw.githubusercontent.com/about-code/glossarify-md/v5.1.0/conf/v5/schema.json",
  "baseDir": "./docs",
  "excludeFiles": [
    "node_modules",
    ".git"
  ],
  "force": false,
  "generateFiles": {
    "listOf": []
  },
  "glossaries": [
    {
      "file": "./glossary.md",
      "termHint": ""
    }
  ],
  "ignoreCase": false,
  "includeFiles": [
    "."
  ],
  "indexing": {
    "groupByHeadingDepth": 6,
    "headingDepths": [
      1,
      2,
      3,
      4,
      5,
      6
    ]
  },
  "i18n": {
    "locale": "en"
  },
  "keepRawFiles": [],
  "linking": {
    "baseUrl": "",
    "paths": "relative",
    "mentions": "all",
    "headingDepths": [
      2,
      3,
      4,
      5,
      6
    ],
    "headingIdAlgorithm": "github",
    "limitByAlternatives": 10
  },
  "outDir": "../docs-glossarified",
  "outDirDropOld": true,
  "reportNotMentioned": false,
  "unified": {}
}
