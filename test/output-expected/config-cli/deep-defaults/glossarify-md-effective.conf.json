{
  "$schema": "../../../../conf/v5/schema.json",
  "baseDir": "{CWD}/output-actual/config-cli/deep-defaults",
  "excludeFiles": [
    "node_modules",
    ".git"
  ],
  "force": false,
  "generateFiles": {
    "listOf": []
  },
  "glossaries": [
    {
      "file": "./glossary.md",
      "termHint": ""
    }
  ],
  "ignoreCase": false,
  "includeFiles": [
    "."
  ],
  "indexing": {
    "groupByHeadingDepth": 6,
    "headingDepths": {
      "1": true,
      "2": true,
      "3": true,
      "4": true,
      "5": true,
      "6": true
    }
  },
  "i18n": {
    "locale": "en"
  },
  "keepRawFiles": [],
  "linking": {
    "baseUrl": "file://localhost",
    "paths": "relative",
    "mentions": "all",
    "headingDepths": {
      "2": true,
      "3": true,
      "4": true,
      "5": true,
      "6": true
    },
    "limitByAlternatives": 10
  },
  "outDir": "{CWD}/output-actual/config-cli/deep-defaults",
  "outDirDropOld": true,
  "reportNotMentioned": false,
  "unified": {
    "settings": {
      "bullet": "-",
      "emphasis": "_",
      "quote": "\""
    }
  },
  "dev": {
    "effectiveConfFile": "./glossarify-md-effective.conf.json"
  }
}
