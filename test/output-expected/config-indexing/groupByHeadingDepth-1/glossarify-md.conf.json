{
  "$schema": "../../../../conf/v5/schema.json",
  "baseDir": ".",
  "linking": {
    "paths": "relative",
    "mentions": "all"
  },
  "includeFiles": [
    "."
  ],
  "excludeFiles": [],
  "indexing": {
    "groupByHeadingDepth": 1
  },
  "generateFiles": {
    "indexFile": {
      "file": "./index.md"
    },
    "listOfFigures": {
      "file": "./figures.md",
      "title": "Figures"
    }
  },
  "glossaries": [
    {
      "file": "./glossary.md"
    }
  ],
  "ignoreCase": false,
  "outDir": "../../../output-actual/config-indexing/groupByHeadingDepth-1",
  "dev": {
    "printInputAst": false,
    "reproducablePaths": true,
    "printOutputAst": false
  }
}
