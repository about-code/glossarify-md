{
  "$schema": "../../../../../conf/v5/schema.json",
  "baseDir": ".",
  "linking": {
    "paths": "relative",
    "mentions": "all"
  },
  "includeFiles": [
    "."
  ],
  "excludeFiles": [],
  "indexing": {
    "groupByHeadingDepth": 0
  },
  "generateFiles": {
    "listOf": [
      {
        "class": "test",
        "file": "./list.md",
        "title": "Test Case",
        "pattern": ":::[ ]?tip Tipp[:]? ([a-zA-Z0-9].*)"
      }
    ]
  },
  "glossaries": [
    {
      "file": "./glossary.md"
    }
  ],
  "ignoreCase": false,
  "outDir": "../../../../output-actual/config-listOf/pattern/tc3-title-capture-group",
  "dev": {
    "printInputAst": false,
    "reproducablePaths": true,
    "printOutputAst": false
  }
}
