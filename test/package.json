    "pll:test_41": "node . --config ./input/config-linking/headingIdAlgorithm/md5/ambiguous-term/glossarify-md.conf.json",
    "pll:test_42": "node . --config ./input/config-linking/headingIdAlgorithm/md5-7/glossarify-md.conf.json",
    "pll:test_43": "node . --config ./input/config-linking/headingIdAlgorithm/sha256-7/one-glossary-with-same-term-twice/glossarify-md.conf.json",
    "pll:test_44": "node . --config ./input/config-linking/headingIdAlgorithm/sha256-7/two-glossaries-with-same-term/glossarify-md.conf.json",
    "pll:test_45": "node . --config ./input/config-linking/headingIdAlgorithm/unsupported/glossarify-md.conf.json",
    "pll:test_46": "node . --config ./input/config-linking/headingAsLink/false/glossarify-md.conf.json",
    "pll:test_47": "node . --config ./input/config-linking/headingIdPandoc/true/glossarify-md.conf.json",
    "pll:test_48": "node . --config ./input/config-linking/limitByAlternatives/positive/glossarify-md.conf.json",
