{
  "private": true,
  "name": "glossarify-md-testsuite",
  "version": "5.0.0",
  "description": "glossarify-md test scripts",
  "author": "Andreas Martin",
  "license": "MIT",
  "main": "../bin/index.js",
  "scripts": {
    "config": "node ../bin/index.js --config",
    "dconfig": "node --inspect-brk ../bin/index.js --config",
    "debug": "node --inspect-brk ../bin/index.js --config ./gitignore.conf.json",
    "debug-diffcheck": "git diff --minimal --color --no-index --ignore-cr-at-eol ./output-expected ./output-actual | node --inspect-brk ../bin/test.js",
    "pretest": "rm -rf ./output-actual",
    "pretest-win": "del /Q .\\output-actual",
    "test-win": "npm run suite",
    "test": "npm run suite",
    "suite": "npm-run-all test_*",
    "at": "npm-run-all",
    "test_00": "node ./conf.spec.js",
    "test_01": "node ./path-tools.spec.js",
    "test_02": "npx . --config ./input/copy-files/glossarify-md.conf.json",
    "test_03": "npx . --config ./input/config-excludeFiles/glossarify-md.conf.json",
    "test_04": "npx . --init > ./input/config-cli/arg-init/conf.gitignore.json && npx . --config ./input/config-cli/arg-init/conf.gitignore.json --shallow \"{ 'outDir': '../../../../output-actual/config-cli/arg-init/' }\"",
    "test_05": "npx . --init > ./output-actual/config-cli/arg-init/minimal.conf.json",
    "test_06": "npx . --init --local > ./output-actual/config-cli/arg-init/minimal-local.conf.json",
    "test_07": "npx . --init --more > ./output-actual/config-cli/arg-init/more.conf.json",
    "test_08": "npx . --init --more --local > ./output-actual/config-cli/arg-init/more-local.conf.json",
    "test_09": "npx . --config ./input/config-cli/arg-shallow/glossarify-md.conf.json --shallow \"{'linking': {'baseUrl': 'cli://localhost', 'paths': 'absolute'}, 'glossaries':[{'file':'./glossary2.md'}] }\"",
    "test_10": "npx . --shallow \"{'baseDir':'./input/config-cli/arg-config-skip', 'outDir':'../../../output-actual/config-cli/arg-config-skip', 'includeFiles':['.'], 'glossaries':[{'file':'./glossary.md'}] }\"",
    "test_11": "npx . --config ./input/config-cli/arg-deep/glossarify-md.conf.json --deep \"{'glossaries':[{'file':'./glossary2.md'}]}\"",
    "test_12": "npx . --config ./input/config-cli/deep-defaults/glossarify-md.conf.json",
    "test_13": "npx . --config ./input/markdown/glossarify-md.conf.json",
    "test_14": "npx . --config ./input/markdown-extensions/glossarify-md.conf.json",
    "test_15": "npx . --config ./input/config-indexing/headingDepths-2-5/glossarify-md.conf.json",
    "test_16": "npx . --config ./input/config-indexing/groupByHeadingDepth-missing/glossarify-md.conf.json",
    "test_17": "npx . --config ./input/config-indexing/groupByHeadingDepth-0/glossarify-md.conf.json",
    "test_18": "npx . --config ./input/config-indexing/groupByHeadingDepth-1/glossarify-md.conf.json",
    "test_19": "npx . --config ./input/config-indexing/groupByHeadingDepth-2/glossarify-md.conf.json",
    "test_20": "npx . --config ./input/config-reportNotMentioned/true/glossarify-md.conf.json",
    "test_21": "npx . --config ./input/config-reportNotMentioned/false/glossarify-md.conf.json",
    "test_22": "npx . --config ./input/term-descriptions/glossarify-md.conf.json",
    "test_23": "npx . --config ./input/aliases-and-synonyms/glossarify-md.conf.json",
    "test_24": "npx . --config ./input/substring_behavior/glossarify-md.conf.json",
    "test_25": "npx . --config ./input/config-ignoreCase-true/glossarify-md.conf.json",
    "test_26": "npx . --config ./input/config-ignoreCase-false/glossarify-md.conf.json",
    "test_27": "npx . --config ./input/config-glossaries/default/glossarify-md.conf.json",
    "test_28": "npx . --config ./input/config-glossaries/file-paths/glossarify-md.conf.json",
    "test_29": "npx . --config ./input/config-glossaries/file-glob/glossarify-md.conf.json",
    "test_30": "npx . --config ./input/config-glossaries/file-glob-and-index/glossarify-md.conf.json",
    "test_31": "npx . --config ./input/config-glossaries/sort-default/glossarify-md.conf.json",
    "test_32": "npx . --config ./input/config-glossaries/sort-i18n-compare-numeric/glossarify-md.conf.json",
    "test_33": "npx . --config ./input/config-glossaries/sort-i18n-de/glossarify-md.conf.json",
    "test_34": "npx . --config ./input/config-linking/headingDepths-2-4/glossarify-md.conf.json",
    "test_35": "npx . --config ./input/config-linking/baseUrl/glossarify-md.conf.json",
    "test_36": "npx . --config ./input/config-linking/paths-relative/glossarify-md.conf.json",
    "test_37": "npx . --config ./input/config-linking/paths-absolute-baseUrl/glossarify-md.conf.json",
    "test_38": "npx . --config ./input/config-linking/mentions-first-in-paragraph/glossarify-md.conf.json",
    "test_39": "npx . --config ./input/config-linking/by-heading-id/glossarify-md.conf.json",
    "test_40": "npx . --config ./input/config-linking/limitByAlternatives/positive/glossarify-md.conf.json",
    "test_41": "npx . --config ./input/config-linking/limitByAlternatives/negative/glossarify-md.conf.json",
    "test_42": "npx . --config ./input/config-indexFile/glossary-in-subdir/glossarify-md.conf.json",
    "test_43": "npx . --config ./input/config-indexFile/index-in-subdir/glossarify-md.conf.json",
    "test_44": "npx . --config ./input/config-indexFile/term-link-to-section/glossarify-md.conf.json",
    "test_45": "npx . --config ./input/config-indexFile/multiple-glossaries/glossarify-md.conf.json",
    "test_46": "npx . --config ./input/config-indexFile/config-title/glossarify-md.conf.json",
    "test_47": "npx . --config ./input/config-indexFile/issue-75-formatted-headings/glossarify-md.conf.json",
    "test_48": "npx . --config ./input/count-term-occurrences/glossarify-md.conf.json",
    "test_49": "npx . --config ./input/config-listOfFigures/image-reference/glossarify-md.conf.json",
    "test_50": "npx . --config ./input/config-listOfFigures/option-title-missing/glossarify-md.conf.json",
    "test_51": "npx . --config ./input/config-listOfFigures/option-with-listOf-combined/glossarify-md.conf.json",
    "test_52": "npx . --config ./input/config-listOfTables/glossarify-md.conf.json",
    "test_53": "npx . --config ./input/config-listOf/glossarify-md.conf.json",
    "test_54": "npx . --config ./input/config-unified/internal/glossarify-md.conf.json",
    "test_55": "npx . --config ./input/config-unified/external/glossarify-md.conf.json",
    "postsuite": "git diff --minimal --color --no-index --ignore-cr-at-eol ./output-expected ./output-actual | node ../bin/test.js",
    "postat": "git diff --minimal --color --no-index --ignore-cr-at-eol ./output-expected ./output-actual | node ../bin/test.js",
    "new-baseline": "rm -r ./output-expected && cp -r ./output-actual ./output-expected",
    "new-baseline-win": "del /Q .\\output-expected && rename .\\output-actual .\\output-expected",
    "commit-baseline": "npm test && git reset && git add './package.json' './input' './output-expected' && git commit -m 'test: New baseline.'"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "remark-frontmatter": "^3.0.0"
  }
}
