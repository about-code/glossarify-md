    "pll:test_67": "node . --config ./input/config-indexFile/config-title/glossarify-md.conf.json",
    "pll:test_68": "node . --config ./input/config-indexFile/issue-75-formatted-headings/glossarify-md.conf.json",
    "pll:test_69": "node . --config ./input/config-indexFiles/flat-dir/glossarify-md.conf.json",
    "pll:test_70": "node . --config ./input/config-indexFiles/154-deep-dir/glossarify-md.conf.json",
    "pll:test_71": "node . --config ./input/term-count-occurrences/glossarify-md.conf.json",
    "pll:test_72": "node . --config ./input/config-listOfFigures/image-reference/glossarify-md.conf.json",
    "pll:test_73": "node . --config ./input/config-listOfFigures/option-title-missing/glossarify-md.conf.json",
    "pll:test_74": "node . --config ./input/config-listOfFigures/option-with-listOf-combined/glossarify-md.conf.json",
