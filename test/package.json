{
  "private": true,
  "name": "glossarify-md-testsuite",
  "version": "6.0.0",
  "description": "glossarify-md test scripts",
  "author": "Andreas Martin",
  "license": "MIT",
  "main": "../bin/index.js",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "at": "npm-run-all",
    "config": "node ../bin/index.js --config",
    "dconfig": "node --inspect-brk ../bin/index.js --config",
    "debug": "node --inspect-brk ../bin/index.js --config ./gitignore.conf.json",
    "debug-diffcheck": "git diff --minimal --color --no-index --ignore-cr-at-eol ./output-expected ./output-actual | node --inspect-brk difftest.js",
    "pretest": "rm -rf ./output-actual",
    "pretest-win": "del /Q .\\output-actual",
    "test-win": "npm run suite",
    "test": "npm run suite",
    "suite": "run-p suite_* --max-parallel 2 --aggregate-output",
    "suite_seq": "run-s seq:* -l",
    "suite_pll": "run-p pll:* -l --max-parallel 2 --aggregate-output",
    "pll:test_00": "node ./conf.spec.js",
    "pll:test_01": "node ./path-tools.spec.js",
    "seq:test_02": "node . --config ./input/copy-files/glossarify-md.conf.json",
    "seq:test_03": "node . --config ./input/config-excludeFiles/glossarify-md.conf.json",
    "seq:test_04": "node . --init > ./input/config-cli/arg-init/conf.gitignore.json && node . --config ./input/config-cli/arg-init/conf.gitignore.json --shallow \"{ 'outDir': '../../../../output-actual/config-cli/arg-init/' }\"",
    "seq:test_05": "node . --init > ./output-actual/config-cli/arg-init/minimal.conf.json",
    "seq:test_06": "node . --init --local > ./output-actual/config-cli/arg-init/minimal-local.conf.json",
    "seq:test_07": "node . --init --more > ./output-actual/config-cli/arg-init/more.conf.json",
    "seq:test_08": "node . --init --more --local > ./output-actual/config-cli/arg-init/more-local.conf.json",
    "seq:test_09": "node . --config ./input/config-cli/arg-shallow/glossarify-md.conf.json --shallow \"{'linking': {'baseUrl': 'cli://localhost/', 'paths': 'absolute'}, 'glossaries':[{'file':'./glossary2.md'}] }\"",
    "seq:test_10": "node . --shallow \"{'baseDir':'./input/config-cli/arg-config-skip', 'outDir':'../../../output-actual/config-cli/arg-config-skip', 'includeFiles':['.'], 'glossaries':[{'file':'./glossary.md'}] }\"",
    "pll:test_11": "node . --config ./input/config-cli/arg-deep/glossarify-md.conf.json --deep \"{'glossaries':[{'file':'./glossary2.md'}]}\"",
    "pll:test_12": "node . --config ./input/config-cli/deep-defaults/glossarify-md.conf.json",
    "pll:test_13": "node . --config ./input/markdown/glossarify-md.conf.json",
    "pll:test_14": "node . --config ./input/markdown-extensions/glossarify-md.conf.json",
    "pll:test_15": "node . --config ./input/config-indexing/headingDepths-2-5/glossarify-md.conf.json",
    "pll:test_16": "node . --config ./input/config-indexing/groupByHeadingDepth-missing/glossarify-md.conf.json",
    "pll:test_17": "node . --config ./input/config-indexing/groupByHeadingDepth-0/glossarify-md.conf.json",
    "pll:test_18": "node . --config ./input/config-indexing/groupByHeadingDepth-1/glossarify-md.conf.json",
    "pll:test_19": "node . --config ./input/config-indexing/groupByHeadingDepth-2/glossarify-md.conf.json",
    "pll:test_20": "node . --config ./input/config-reportNotMentioned/true/glossarify-md.conf.json",
    "pll:test_21": "node . --config ./input/config-reportNotMentioned/false/glossarify-md.conf.json",
    "pll:test_22": "node . --config ./input/term-descriptions/glossarify-md.conf.json",
    "pll:test_23": "node . --config ./input/term-aliases-and-synonyms/glossarify-md.conf.json",
    "pll:test_24": "node . --config ./input/term-substring-dissection/glossarify-md.conf.json",
    "pll:test_25": "node . --config ./input/config-ignoreCase-true/glossarify-md.conf.json",
    "pll:test_26": "node . --config ./input/config-ignoreCase-false/glossarify-md.conf.json",
    "pll:test_27": "node . --config ./input/config-glossaries/default/glossarify-md.conf.json",
    "pll:test_28": "node . --config ./input/config-glossaries/export-and-glossary-uri/glossarify-md.conf.json",
    "pll:test_29": "node . --config ./input/config-glossaries/file-paths/glossarify-md.conf.json",
    "pll:test_30": "node . --config ./input/config-glossaries/file-glob/glossarify-md.conf.json",
