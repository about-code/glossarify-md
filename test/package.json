{
  "private": true,
  "name": "glossarify-md-testsuite",
  "version": "6.0.0",
  "description": "glossarify-md test scripts",
  "author": "Andreas Martin",
  "license": "MIT",
  "main": "../bin/index.js",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "at": "npm-run-all",
    "config": "node ../bin/index.js --config",
    "dconfig": "node --inspect-brk ../bin/index.js --config",
    "debug": "node --inspect-brk ../bin/index.js --config ./gitignore.conf.json",
    "debug-diffcheck": "git diff --minimal --color --no-index --ignore-cr-at-eol ./output-expected ./output-actual | node --inspect-brk difftest.js",
    "pretest": "rm -rf ./output-actual",
    "pretest-win": "del /Q .\\output-actual",
    "test-win": "npm run suite",
    "test": "npm run suite",
    "suite": "run-p suite_* --max-parallel 2 --aggregate-output",
    "suite_00": "npm-run-all -l test_0*",
    "suite_01": "npm-run-all -l test_1*",
    "suite_02": "npm-run-all -l test_2*",
    "suite_03": "npm-run-all -l test_3*",
    "suite_04": "npm-run-all -l test_4*",
    "suite_05": "npm-run-all -l test_5*",
    "suite_06": "npm-run-all -l test_6*",
    "suite_07": "npm-run-all -l test_7*",
    "test_00": "node ./conf.spec.js",
    "test_01": "node ./path-tools.spec.js",
    "test_02": "node . --config ./input/copy-files/glossarify-md.conf.json",
    "test_03": "node . --config ./input/config-excludeFiles/glossarify-md.conf.json",
    "test_04": "node . --init > ./input/config-cli/arg-init/conf.gitignore.json && node . --config ./input/config-cli/arg-init/conf.gitignore.json --shallow \"{ 'outDir': '../../../../output-actual/config-cli/arg-init/' }\"",
    "test_05": "node . --init > ./output-actual/config-cli/arg-init/minimal.conf.json",
    "test_06": "node . --init --local > ./output-actual/config-cli/arg-init/minimal-local.conf.json",
    "test_07": "node . --init --more > ./output-actual/config-cli/arg-init/more.conf.json",
    "test_08": "node . --init --more --local > ./output-actual/config-cli/arg-init/more-local.conf.json",
    "test_09": "node . --config ./input/config-cli/arg-shallow/glossarify-md.conf.json --shallow \"{'linking': {'baseUrl': 'cli://localhost', 'paths': 'absolute'}, 'glossaries':[{'file':'./glossary2.md'}] }\"",
    "test_10": "node . --shallow \"{'baseDir':'./input/config-cli/arg-config-skip', 'outDir':'../../../output-actual/config-cli/arg-config-skip', 'includeFiles':['.'], 'glossaries':[{'file':'./glossary.md'}] }\"",
    "test_11": "node . --config ./input/config-cli/arg-deep/glossarify-md.conf.json --deep \"{'glossaries':[{'file':'./glossary2.md'}]}\"",
    "test_12": "node . --config ./input/config-cli/deep-defaults/glossarify-md.conf.json",
    "test_13": "node . --config ./input/markdown/glossarify-md.conf.json",
    "test_14": "node . --config ./input/markdown-extensions/glossarify-md.conf.json",
    "test_15": "node . --config ./input/config-indexing/headingDepths-2-5/glossarify-md.conf.json",
    "test_16": "node . --config ./input/config-indexing/groupByHeadingDepth-missing/glossarify-md.conf.json",
    "test_17": "node . --config ./input/config-indexing/groupByHeadingDepth-0/glossarify-md.conf.json",
    "test_18": "node . --config ./input/config-indexing/groupByHeadingDepth-1/glossarify-md.conf.json",
    "test_19": "node . --config ./input/config-indexing/groupByHeadingDepth-2/glossarify-md.conf.json",
    "test_20": "node . --config ./input/config-reportNotMentioned/true/glossarify-md.conf.json",
    "test_21": "node . --config ./input/config-reportNotMentioned/false/glossarify-md.conf.json",
    "test_22": "node . --config ./input/term-descriptions/glossarify-md.conf.json",
    "test_23": "node . --config ./input/aliases-and-synonyms/glossarify-md.conf.json",
    "test_24": "node . --config ./input/substring_behavior/glossarify-md.conf.json",
    "test_25": "node . --config ./input/config-ignoreCase-true/glossarify-md.conf.json",
    "test_26": "node . --config ./input/config-ignoreCase-false/glossarify-md.conf.json",
    "test_27": "node . --config ./input/config-glossaries/default/glossarify-md.conf.json",
    "test_28": "node . --config ./input/config-glossaries/file-paths/glossarify-md.conf.json",
    "test_29": "node . --config ./input/config-glossaries/file-glob/glossarify-md.conf.json",
    "test_30": "node . --config ./input/config-glossaries/file-glob-and-index/glossarify-md.conf.json",
    "test_31": "node . --config ./input/config-glossaries/sort-default/glossarify-md.conf.json",
    "test_32": "node . --config ./input/config-glossaries/sort-i18n-compare-numeric/glossarify-md.conf.json",
    "test_33": "node . --config ./input/config-glossaries/sort-i18n-de/glossarify-md.conf.json",
    "test_34": "node . --config ./input/config-linking/headingDepths-2-4/glossarify-md.conf.json",
    "test_35": "node . --config ./input/config-linking/baseUrl/glossarify-md.conf.json",
    "test_36": "node . --config ./input/config-linking/paths-relative/glossarify-md.conf.json",
    "test_37": "node . --config ./input/config-linking/paths-absolute-baseUrl/glossarify-md.conf.json",
    "test_38": "node . --config ./input/config-linking/paths-absolute-baseUrl-blank/glossarify-md.conf.json",
    "test_39": "node . --config ./input/config-linking/mentions-first-in-paragraph/glossarify-md.conf.json",
    "test_40": "node . --config ./input/config-linking/limitByAlternatives/positive/glossarify-md.conf.json",
    "test_41": "node . --config ./input/config-linking/limitByAlternatives/negative/glossarify-md.conf.json",
    "test_42": "node . --config ./input/config-linking/limitByAlternatives/zero/glossarify-md.conf.json",
    "test_43": "node . --config ./input/config-linking/heading-id-algorithm/md5/ambiguous-term/glossarify-md.conf.json",
    "test_44": "node . --config ./input/config-linking/heading-id-algorithm/md5-7/glossarify-md.conf.json",
    "test_45": "node . --config ./input/config-linking/headingAsLink/false/glossarify-md.conf.json",
    "test_46": "node . --config ./input/config-linking/headingIdPandoc/true/glossarify-md.conf.json",
    "test_47": "node . --config ./input/config-linking/byReferenceDefinition/false/glossarify-md.conf.json",
    "test_48": "node . --config ./input/config-linking/paths-absolute-pathComponents/path/glossarify-md.conf.json",
    "test_49": "node . --config ./input/config-linking/paths-absolute-pathComponents/path-file/glossarify-md.conf.json",
    "test_50": "node . --config ./input/config-linking/paths-absolute-pathComponents/path-file-ext/glossarify-md.conf.json",
    "test_51": "node . --config ./input/config-linking/paths-relative-pathComponents/path/glossarify-md.conf.json",
    "test_52": "node . --config ./input/config-linking/paths-relative-pathComponents/path-file/glossarify-md.conf.json",
    "test_53": "node . --config ./input/config-linking/paths-relative-pathComponents/path-file-ext/glossarify-md.conf.json",
    "test_54": "node . --config ./input/config-indexFile/glossary-in-subdir/glossarify-md.conf.json",
    "test_55": "node . --config ./input/config-indexFile/index-in-subdir/glossarify-md.conf.json",
    "test_56": "node . --config ./input/config-indexFile/term-link-to-section/glossarify-md.conf.json",
    "test_57": "node . --config ./input/config-indexFile/multiple-glossaries/glossarify-md.conf.json",
    "test_58": "node . --config ./input/config-indexFile/config-title/glossarify-md.conf.json",
    "test_59": "node . --config ./input/config-indexFile/issue-75-formatted-headings/glossarify-md.conf.json",
    "test_60": "node . --config ./input/config-indexFiles/flat-dir/glossarify-md.conf.json",
    "test_61": "node . --config ./input/config-indexFiles/154-deep-dir/glossarify-md.conf.json",
    "test_62": "node . --config ./input/count-term-occurrences/glossarify-md.conf.json",
    "test_63": "node . --config ./input/config-listOfFigures/image-reference/glossarify-md.conf.json",
    "test_64": "node . --config ./input/config-listOfFigures/option-title-missing/glossarify-md.conf.json",
    "test_65": "node . --config ./input/config-listOfFigures/option-with-listOf-combined/glossarify-md.conf.json",
    "test_66": "node . --config ./input/config-listOfTables/glossarify-md.conf.json",
    "test_67": "node . --config ./input/config-listOf/basic/glossarify-md.conf.json",
    "test_68": "node . --config ./input/config-listOf/pattern-tc1-text-paragraph/glossarify-md.conf.json",
    "test_69": "node . --config ./input/config-listOf/pattern-tc2-formatted-paragraph/glossarify-md.conf.json",
    "test_70": "node . --config ./input/config-listOf/pattern-tc3-title-capture-group/glossarify-md.conf.json",
    "test_71": "node . --config ./input/config-listOf/pattern-tc4-sanitization/glossarify-md.conf.json",
    "test_72": "node . --config ./input/config-unified/internal/glossarify-md.conf.json",
    "test_73": "node . --config ./input/config-unified/external/glossarify-md.conf.json",
    "test_74": "node . --config ./input/pandoc/config-linking/heading-id/glossarify-md.conf.json",
    "test_75": "node . --config ./input/pandoc/config-linking/heading-id-algorithm/mixed-with-pandoc-ids/glossarify-md.conf.json",
    "postsuite": "git diff --minimal --color --no-index --ignore-cr-at-eol ./output-expected ./output-actual | node difftest.js",
    "postat": "git diff --minimal --color --no-index --ignore-cr-at-eol ./output-expected ./output-actual | node difftest.js",
    "new-baseline": "rm -r ./output-expected && cp -r ./output-actual ./output-expected",
    "new-baseline-win": "del /Q .\\output-expected && rename .\\output-actual .\\output-expected",
    "commit-baseline": "npm test && git reset && git add './package.json' './input' './output-expected' && git commit -m 'test: New baseline.'"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "remark-frontmatter": "^3.0.0"
  }
}
