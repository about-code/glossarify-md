    "pll:test_85": "node . --config ./input/pandoc/config-glossaries/uri-from-heading-id/glossarify-md.conf.json",
    "pll:test_86": "node . --config ./input/term-uris/uri-none-baseUrl-none-paths-relative/glossarify-md.conf.json",
    "pll:test_87": "node . --config ./input/term-uris/uri-none-baseUrl-none-paths-absolute/glossarify-md.conf.json",
    "pll:test_88": "node . --config ./input/term-uris/uri-none-baseUrl-fallback-paths-relative/glossarify-md.conf.json",
    "pll:test_89": "node . --config ./input/term-uris/uri-none-baseUrl-fallback-paths-absolute/glossarify-md.conf.json",
    "pll:test_90": "node . --config ./input/term-uris/uri-one-headingIdAlgorithm-md5/glossarify-md.conf.json",
    "pll:test_91": "node . --config ./input/term-uris/uri-some-baseUrl-fallback/glossarify-md.conf.json",
    "postsuite": "git diff --minimal --color --no-index --ignore-cr-at-eol ./output-expected ./output-actual | node difftest.js",
    "postat": "git diff --minimal --color --no-index --ignore-cr-at-eol ./output-expected ./output-actual | node difftest.js",
    "new-baseline": "rm -r ./output-expected && cp -r ./output-actual ./output-expected",
    "new-baseline-win": "del /Q .\\output-expected && rename .\\output-actual .\\output-expected",
    "commit-baseline": "npm test && git reset && git add './package.json' './input' './output-expected' && git commit -m 'test: New baseline.'"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "remark-frontmatter": "^3.0.0"
  }
}
