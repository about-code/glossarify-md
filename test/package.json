    "pll:test_61": "node . --config ./input/config-linking/paths-relative-pathComponents/path-file-ext/glossarify-md.conf.json",
    "pll:test_62": "node . --config ./input/config-linking/paths-relative-pathComponents/none/glossarify-md.conf.json",
    "pll:test_63": "node . --config ./input/config-indexFile/glossary-in-subdir/glossarify-md.conf.json",
